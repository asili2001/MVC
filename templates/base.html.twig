<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Welcome!{% endblock %}</title>
        <link rel="icon" href="data:image/svg+xml,<svg xmlns=%22http://www.w3.org/2000/svg%22 viewBox=%220 0 128 128%22><text y=%221.2em%22 font-size=%2296%22>⚫️</text></svg>">
        <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.4.0/css/all.min.css" />
        {# Run `composer require symfony/webpack-encore-bundle` to start using Symfony UX #}
        {% block stylesheets %}
            {{ encore_entry_link_tags('app') }}
        {% endblock %}

        {% block javascripts %}
            {{ encore_entry_script_tags('app') }}
        {% endblock %}
    </head>
    <body>
        <header>
            <nav>
                <ul>
                    <a href="{{ path('home') }}"><li>Hem</li></a>
                    <a href="{{ path('about') }}"><li>Om MVC</li></a>
                    <a href="{{ path('report') }}"><li>Report</li></a>
                    <a href="{{ path('lucky') }}"><li>Your Lucky Day</li></a>
                    <a href="{{ path('playingCardHome') }}"><li>Playing Cards</li></a>
                    <a href="{{ path('cardGame') }}"><li>BlackJack</li></a>
                    <a href="{{ path('playingCardHomeJson') }}"><li>Playing cards Api</li></a>
                    <a href="{{ path('app_library') }}"><li>Library</li></a>
                </ul>
            </nav>
        </header>
        {% block body %}{% endblock %}

        <div class="footer">
            Copyright: Ahmad Asili
        </div>
    </body>
</html>
